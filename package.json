{
  "name": "bassclefnights",
  "version": "1.0.0",
  "description": "Bass Clef Nights website",
  "main": "build/bundle.js",
  "engines": {
    "node": "9.2.0"
  },
  "scripts": {
    "start": "node build/bundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "heroku-postbuild": "webpack --config webpack.server.js && webpack --config webpack.client.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-css-import-to-string": "0.0.2",
    "babel-preset-react-plus": "^1.3.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "express-validator": "^4.3.0",
    "mailgun-js": "^0.14.2",
    "moment": "2.20.1",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.1.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.5.6",
    "serialize-javascript": "^1.4.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1"
  },
  "author": "Patrick Williams & Eduardo Ruiz",
  "license": "MIT"
}
